{
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "parser": "babel-eslint",
  "rules": {
    "quotes": [2, "single"],
    "eqeqeq": [2, "smart"],
    "strict": 0,
    "indent": [2, 2],
    "camelcase": [0],
    "consistent-return": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0]
  }
}
